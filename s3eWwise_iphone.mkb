#!/usr/bin/env mkb
# Builder mkb file for the s3eWwise extension on iphone
platform IPHONE

commands
{
	"setup_libs.py"
}

options
{
    lib
    output-name="lib/iphone/libs3eWwise.a"
    output-name-debug="lib/iphone/libs3eWwise_d.a"
    arm-arch=armv6
}
define S3E_BUILD_EXT

subproject s3eWwise_build
